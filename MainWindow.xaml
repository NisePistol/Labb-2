<Window x:Class="Labb_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="250" Background="#EEE">
        <StackPanel Margin="5">

            <Label Margin="5">Slumpa fram ett tal 1-100</Label>

            <Button Margin="5" Click="Slumpa">Slumpa</Button>

            <TextBox Margin="5" Name="slumpTal" IsReadOnly="True"></TextBox>
            
        </StackPanel>
</Window>
